{
  "name": "react-horizontalcharts",
  "version": "0.2.1",
  "author": "Andrea Giovanni Bianchessi <andrea.g.bianchessi@gmail.com> (https://www.horizontalcharts.org)",
  "description": "React Horizontal Charts: React components for HorizontalCharts library",
  "private": false,
  "keywords": [
    "react",
    "charts",
    "charting",
    "horizontal",
    "horizontal charts",
    "realtime",
    "bar",
    "bar charts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agbianchessi/React_HorizontalCharts.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agbianchessi/React_HorizontalCharts/issues"
  },
  "homepage": "https://www.horizontalcharts.org",
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "transpile": "NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "copy": "cp LICENSE README.md dist/",
    "pack": "webpack --mode production",
    "build": "npm run clean && npm run transpile && npm run copy",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
